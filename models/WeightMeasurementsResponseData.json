{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "WeightMeasurementInsertRequestData",
    "type": "object",
    "properties": {
        "measurements": {"type": "array",
        "items": {
            "type": "object",
            "properties": {
                "userId": {"type": "string"},
                "measurementId": {"type": "string"},
                "timestamp": {"type": "number"},
                "date": {"type": "string"},
                "comment": {"type": "string"},
                "measurements": {"type": "object", "properties": {
                    "weight": {"type": "number"},
                    "bodyFatPercentage": {"type": "number"},
                    "waterPercentage": {"type": "number"},
                    "muscleMassPercentage": {"type": "number"},
                    "bonePercentage": {"type": "number"},
                    "energyExpenditure": {"type": "number"}
                }, "required": ["weight"]}    
            },
            "required": ["userId", "measurementId", "timestamp", "date", "comment", "measurements"]}
        }
    }
}